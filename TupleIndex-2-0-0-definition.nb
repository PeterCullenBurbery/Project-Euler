Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"TupleIndex", "[", "tuple_List", "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "run", ",", "max", ",", "len", ",", "first", ",", "div", ",", "grow", 
       ",", " ", "switch", ",", "index"}], "}"}], ",", "\[IndentingNewLine]", 
     
     RowBox[{
      RowBox[{"run", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"max", "=", 
       RowBox[{"Max", "[", "tuple", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"max", "\[Equal]", "0"}], ",", "\[IndentingNewLine]", 
        RowBox[{"index", "=", "1"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"len", "=", 
          RowBox[{"Length", "[", "tuple", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"first", "=", 
          RowBox[{
           RowBox[{"Position", "[", 
            RowBox[{"tuple", ",", "max"}], "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"div", "=", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"max", "+", "1"}], ")"}], "^", "n"}], " ", "-", " ", 
             RowBox[{
              RowBox[{"(", "max", ")"}], "^", "n"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"n", ",", 
              RowBox[{"len", "-", "1"}], ",", "0", ",", 
              RowBox[{"-", "1"}]}], "}"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"index", "=", 
          RowBox[{
           RowBox[{"max", "^", "len"}], "+", "1", "+", 
           RowBox[{"FromDigits", "[", 
            RowBox[{
             RowBox[{"Take", "[", 
              RowBox[{"tuple", ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"first", "+", "1"}], ",", "len"}], "}"}]}], "]"}], 
             ",", 
             RowBox[{"max", "+", "1"}]}], "]"}], "+", 
           RowBox[{
            RowBox[{"Drop", "[", 
             RowBox[{"tuple", ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"first", "+", "1"}], ",", "len"}], "}"}]}], "]"}], 
            ".", 
            RowBox[{"Take", "[", 
             RowBox[{"div", ",", "first"}], "]"}]}]}]}]}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", "index"}]}], 
    "]"}]}], ";"}]], "Input",
