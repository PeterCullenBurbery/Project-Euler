Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"OrderedTupleFromIndex", "[", 
    RowBox[{"index_Integer", ",", "len_Integer"}], "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"tuple", ",", "total", ",", " ", "x"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"tuple", "=", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", "len", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"total", "=", "index"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"tuple", "[", 
           RowBox[{"[", "in", "]"}], "]"}], "=", 
          RowBox[{
           RowBox[{"Ceiling", "[", 
            RowBox[{"x", "/.", 
             RowBox[{"Flatten", "[", "\[IndentingNewLine]", 
              RowBox[{"NSolve", "[", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"Product", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"x", "-", "n", "+", "1"}], ")"}], "/", "n"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"n", ",", "1", ",", "in"}], "}"}]}], "]"}], 
                   "\[Equal]", "total"}], ",", 
                  RowBox[{"x", ">", "0"}]}], "}"}], ",", "x"}], "]"}], 
              "]"}]}], "]"}], "-", "1"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"total", "=", 
          RowBox[{"total", "-", 
           RowBox[{"Binomial", "[", 
            RowBox[{
             RowBox[{"tuple", "[", 
              RowBox[{"[", "in", "]"}], "]"}], ",", "in"}], "]"}]}]}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"in", ",", "len", ",", "1", ",", 
          RowBox[{"-", "1"}]}], "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"tuple", "-", 
       RowBox[{"Range", "[", 
        RowBox[{"0", ",", 
         RowBox[{"len", "-", "1"}]}], "]"}]}]}]}], "]"}]}], ";"}]], "Input",
