  TagBox[
   FormBox[
    RowBox[{
     StyleBox["n", "TI"], "=", 
     RowBox[{
      FormBox[
       UnderoverscriptBox["\[Sum]", 
        RowBox[{
         StyleBox["j", "TI"], "=", "0"}], 
        RowBox[{
         StyleBox["k", "TI"], "-", "1"}]],
       TraditionalForm], 
      TemplateBox[{
        FormBox[
         RowBox[{
           SubscriptBox[
            StyleBox["a", "TI"], 
